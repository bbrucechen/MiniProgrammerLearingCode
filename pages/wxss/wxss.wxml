<!-- 设置样式三种方法 -->
<!-- 1.内联式 style属性 -->
<view class="box" style="font-size:24px;">hahah</view>

<!-- 2.外联式 通过wxss -->
<view>123</view>

<!-- 3.全局样式 写在app.wxss中 -->
<view class="container">hehehe1</view>    

<!-- wxss中的单位：rpx 响应式px -->
<view style="width:200rpx;height:200rpx;background-color:red;"></view>

